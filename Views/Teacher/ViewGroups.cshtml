@model List<Dictionary<string, object>>

@{
    ViewData["Title"] = "ViewGroups";
}

<h1>ViewGroup</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Name
            </th>
            <th>
                Group_type
            </th>
            <th>
                Supervisor_id
            </th>
            <th></th>
        </tr>
    </thead>
<tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @item["Name"]
            </td>
            <td>
                @item["Group_type"]
            </td>
            <td>
                @item["Supervisor_id"]
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item["Id"]">Edit</a> |
                <a asp-action="DetailsGroup" asp-route-id="@item["Id"]">Details</a> |
                <a asp-action="Delete" asp-route-id="@item["Id"]">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
