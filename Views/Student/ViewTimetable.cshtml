@model List<Dictionary<string, object>>
@{
    ViewData["Title"] = "ViewTimetable1";
}

<h1>@ViewData["Title"]</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Group_Name
            </th>
            <th>
                Group_type
            </th>
            <th>
                Supervisor_Name
            </th>
            <th>
                Week_day
            </th>
            <th>
                Time
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                <a asp-action="DetailsGroup" asp-route-id="@item["Id"]">@item["Group_Name"]</a>
            </td>
            <td>
                @item["Group_type"]
            </td>
            <td>
                @item["Supervisor_Name"]
            </td>
            <td>
                @item["Week_day"]
            </td>
            <td>
                @item["Time"]
            </td>
        </tr>
        }
    </tbody>
</table>